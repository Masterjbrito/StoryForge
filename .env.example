VITE_AGENT_PROVIDER=mock

# Core Foundry connection
VITE_FOUNDRY_ENDPOINT=
VITE_FOUNDRY_API_KEY=
VITE_FOUNDRY_PROJECT_ID=

# Modes:
# - single-endpoint: one endpoint handles all agent types
# - agent-id: each agent has its own ID + URL template
VITE_FOUNDRY_MODE=single-endpoint

# Auth:
# - bearer: Authorization: Bearer <key>
# - api-key: <header>=<key>
VITE_FOUNDRY_AUTH_MODE=bearer
VITE_FOUNDRY_API_KEY_HEADER=api-key

# Used only when VITE_FOUNDRY_MODE=agent-id
# Placeholders allowed: {projectId} {agentId} {agentType}
VITE_FOUNDRY_AGENT_URL_TEMPLATE=

# Agent IDs (use when VITE_FOUNDRY_MODE=agent-id)
VITE_FOUNDRY_AGENT_CONTEXT_INGESTOR_ID=
VITE_FOUNDRY_AGENT_QUESTIONNAIRE_DISCOVERY_ID=
VITE_FOUNDRY_AGENT_REQUIREMENTS_GENERATOR_ID=
VITE_FOUNDRY_AGENT_ACCEPTANCE_CRITERIA_ID=
VITE_FOUNDRY_AGENT_TEST_DESIGN_ID=
VITE_FOUNDRY_AGENT_QUALITY_GATE_ID=
VITE_FOUNDRY_AGENT_VERSIONING_DIFF_ID=
VITE_FOUNDRY_AGENT_EXPORT_ID=
VITE_FOUNDRY_AGENT_AUDIT_LOGGING_ID=
